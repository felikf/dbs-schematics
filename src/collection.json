{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "module": {
      "description": "DBS module schematic.",
      "factory": "./module/index#module",
      "schema": "./module/schema.json"
    },
    "info-component": {
      "description": "DBS info component schematic.",
      "factory": "./info-component/index#infoComponent",
      "schema": "./info-component/schema.json"
    },
    "table-component": {
      "description": "DBS table component schematic.",
      "factory": "./table-component/index#tableComponent",
      "schema": "./table-component/schema.json"
    },
    "service": {
      "description": "DBS service schematic.",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "error-configuration": {
      "description": "DBS error configuration schematic.",
      "factory": "./error-configuration/index#errorConfiguration",
      "schema": "./error-configuration/schema.json"
    },
    "all": {
      "description": "DBS complete module with component, service and error configuration schematic.",
      "factory": "./all/index#all",
      "schema": "./all/schema.json"
    }
  }
}
